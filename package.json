        "@vue/tsconfig": "^0.4.0",
        "cz-customizable": "^7.0.0",
        "eslint": "^8.49.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-define-config": "^1.23.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-prettier": "5.0.0",
        "eslint-plugin-vue": "^9.17.0",
        "husky": "^8.0.3",
        "jsdom": "^22.1.0",
        "lint-staged": "^14.0.1",
        "prettier": "^3.0.3",
        "rollup-plugin-visualizer": "^5.9.2",
