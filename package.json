        "crypto-js": "^4.1.1",
        "dayjs": "^1.11.9",
        "docx-preview": "^0.1.18",
        "echarts": "^5.4.3",
        "element-plus": "^2.3.14",
        "exceljs": "^4.3.0",
        "file-saver": "^2.0.5",
        "html2canvas": "^1.4.1",
        "jsbarcode": "^3.11.5",
        "lodash-es": "^4.17.21",
        "luckyexcel": "^1.0.1",
        "mitt": "^3.0.1",
        "moment": "^2.29.4",
        "nprogress": "^0.2.0",
        "pinia": "^2.1.6",
        "pnpm": "^9.0.4",
        "print-js": "^1.6.0",
        "qrcode.vue": "^3.4.1",
        "store2": "^2.14.2",
        "vue": "^3.3.4",
        "vue-router": "^4.2.4",
        "vuedraggable": "^4.1.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.7.1",
        "@commitlint/config-conventional": "^17.7.0",
        "@tsconfig/node18": "^18.2.2",
        "@types/file-saver": "^2.0.5",
        "@types/jsdom": "^21.1.3",
        "@types/lodash-es": "^4.17.9",
        "@types/node": "^20.6.2",
        "@typescript-eslint/eslint-plugin": "^6.7.0",
        "@typescript-eslint/parser": "^6.7.0",
        "@vitejs/plugin-legacy": "^4.1.1",
        "@vitejs/plugin-vue": "^4.3.4",
        "@vitejs/plugin-vue-jsx": "^3.0.2",
        "@vue/eslint-config-prettier": "^8.0.0",
